<?xml version="1.0" encoding="UTF-8"?>
<sequence xmlns="http://ws.apache.org/ns/synapse"
          name="twitterSequence"
          onError="fault"
          trace="disable">
   <in>
      <log level="full">
         <property name="test" value="log before send for twitterSequence"/>
      </log>
      <payloadFactory media-type="json">
         <format>    "twitterData":"$1"</format>
         <args>
            <arg evaluator="json" expression="&#34;$.text&#34;"/>
         </args>
      </payloadFactory>
      <send>
         <endpoint>
            <http method="POST"
                  uri-template="  http://localhost:9765/endpoints/HTTPReceiver"/>
         </endpoint>
      </send>
   </in>
   <out>
      <send/>
   </out>
   <description>The main sequence for the message mediation</description>
</sequence>
